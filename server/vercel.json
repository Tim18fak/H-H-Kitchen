{
 "version": 2,
 "build": [
  {"src": "index.js", "use": "node@vercel"},
  {"src": "auth.js", "use": "node@vercel"}
 ],
 "routes": [
  {
   "src": "/H(*)",
   "methods": ["POST","GET"],
   "dest": "index.js"
  },
  {
   "src": "/HV(*)",
   "methods": ["POST"],
   "dest": "auth.js"
  },
  {
   "src": "/HA(*)",
   "dest": "admin.js"
  }
 ]
}